@using System
@using AIP.GUI
<table width="100%" frame="border" align="center">
    <colgroup>
        <col width="4%" />
        <col width="48%" />
        <col width="48%" />
    </colgroup>
    <tbody>
        <tr>
            <th align="left" class="bleft btop bright bbottom" width="10">1</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_1"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.Designator) @Raw(Model.Name)<br />@Raw(Model.Coords)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">2</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_2"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.VerticalLimits)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">3</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_3"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.AispaceClassification)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">4</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_4"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.CallsignDetails)<br />@Raw(Model.Language)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">5</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_5"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.TransitionAltitude)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">6</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_6"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.HoursApplicability)</td>
        </tr>
        <tr>
            <th align="left" class="bleft btop bright bbottom">7</th>
		<th align="left" class="bleft btop bright bbottom">@Raw(@Tpl.Text("AD217_7"))</th>
            <td class="bleft btop bright bbottom">@Raw(Model.Remarks)</td>
        </tr>
    </tbody>
</table>