@using System
@using AIP.GUI
@{
    int Col = 6;
    int i = 1;
}

@foreach (var data in Model)
{
    <table frame="border" align="center">
        <colgroup>
            <col width="18%" />
            <col width="16%" />
            <col width="16%" />
            <col width="16%" />
            <col width="16%" />
            <col width="18%" />
        </colgroup>
        <thead>
        <tr class="Table-row-type-1">
            @for (i = 1; i <= Col; i++)
            {
                <th class="bleft btop bright bbottom">@Raw(@Tpl.Text("ENR42_"+@i))</th>
            }
        </tr>
        <tr class="Table-row-type-2">
            @for (i = 1; i <= Col; i++)
            {
                <th class="bleft btop bright bbottom">@i</th>
            }
        </tr>
        </thead>
        <tbody valign="top">
        <tr>
            <td class="bleft btop bright bbottom" colspan="6"><strong>@Raw(data.name)</strong></td>
        </tr>
        @foreach (var subdata in data.Row)
        {
            <tr>
                <td class="bleft btop bright bbottom">
                    <strong class="Cell-body">@Raw(subdata["snsName"])</strong>
                </td>
                <td class="bleft btop bright bbottom">@Raw(subdata["type"])</td>
                <td class="bleft btop bright bbottom">@Raw(subdata["Frequency"])</td>
                <td class="bleft btop bright bbottom">@Raw(subdata["Availability"])</td>
                <td class="bleft btop bright bbottom">@Raw(subdata["Coordinates"])</td>
                <td class="bleft btop bright bbottom">@Raw(subdata["notes"])</td>
            </tr>
            <
        }
        </tbody>
    </table>
}