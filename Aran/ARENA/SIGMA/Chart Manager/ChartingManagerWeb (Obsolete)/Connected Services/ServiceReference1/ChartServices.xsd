  </xs:simpleType>
  <xs:element name="UserPrivilege" nillable="true" type="tns:UserPrivilege" />
  <xs:simpleType name="ChartType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="All" />
      <xs:enumeration value="SID" />
      <xs:enumeration value="STAR" />
      <xs:enumeration value="IAC" />
      <xs:enumeration value="Enroute" />
      <xs:enumeration value="Aerodrome" />
      <xs:enumeration value="A" />
      <xs:enumeration value="PAT" />
      <xs:enumeration value="Area" />
