@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Login Page</title>
    <link rel='stylesheet prefetch' href='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css'>
    <link href="~/MyCssFiles/css/LLsoginCsms.css" rel="stylesheet" />

</head>
<body>
    <div class='login'>
        <div class='login_title'>
            <label style="color:#673434;">Risk User Account</label>
        </div>
        <p id="ErrorMessage" style="color:red;display:none;">Username or Password is incorrect</p>
        <br />
        <div class='login_fields'>
            <div id="UserNamDivArea" class='login_fields__user'>
                <div class='icon'>
                    <img src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/217233/user_icon_copy.png'>
                </div>
                <input id="InputUsername" placeholder='Username' type='text'>
                <div class='validation'>
                    <img src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/217233/tick.png'>
                </div>
            </div>
            <br />
            <div id="PasswAreaDiv" class='login_fields__password'>
                <div class='icon'>
                    <img src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/217233/lock_icon_copy.png'>
                </div>
                <input id="InputPswrd" placeholder='Password' type='password'>
                <div class='validation'>
                    <img src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/217233/tick.png'>
                </div>
            </div>
            <div class='login_fields__submit'>
                <input id="BtnLogin" type="button" value='Log In'>
            </div>
        </div>
    </div>
    <div class='authent'>
        <img src='https://s3-us-west-2.amazonaws.com/s.cdpn.io/217233/puff.svg'>
        <p>Authenticating...</p>
    </div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js'></script>


    <script type="text/javascript">
            $(document).ready(function () {
                //var mychek = null;
                $('input[type="text"],input[type="password"]').focus(function () {
                    $(this).prev().animate({ 'opacity': '1' }, 200);
                });
                $('input[type="text"],input[type="password"]').blur(function () {
                    $(this).prev().animate({ 'opacity': '.5' }, 200);
                });

                $('input[type="text"],input[type="password"]').keyup(function () {
                    if (!$(this).val() == '') {
                        $(this).next().animate({ 'opacity': '1', 'right': '30' }, 200);
                    } else {
                        $(this).next().animate({ 'opacity': '0', 'right': '20' }, 200);
                    }
                });

                var open = 0;
                $('.tab').click(function () {
                    $(this).fadeOut(200, function () {
                        $(this).parent().animate({ 'left': '0' });
                    });
                });
                $(document).keypress(function (e) {
                    if (e.which == 13) {
                        AccessFunction();
                    }
                });
                //var CheckTxt = null;
                $("#BtnLogin").click(function () {
                    AccessFunction();
                });
                function AccessFunction() {

                    var UsernameBox = $("#InputUsername").val();
                    var PasswordBox = $("#InputPswrd").val();

                    if (UsernameBox == "") {
                        $("#InputUsername").css("border-bottom", "2px solid #b50e0e");
                    }
                    if (UsernameBox != "") {
                        $("#InputUsername").css("border-bottom", "2px solid #fab1b1");
                    }
                    if (PasswordBox == "") {
                        $("#InputPswrd").css("border-bottom", "2px solid #b50e0e");
                    }
                    if (PasswordBox != "") {
                        $("#InputPswrd").css("border-bottom", "2px solid #fab1b1");
                    }
                    if (UsernameBox != "" && PasswordBox != "") {

                        //var MyCheckTxt = null;

                        $('.login').addClass('test');
                        $('.login').addClass('testtwo');
                        $(".authent").show().animate({ right: -320 }, { easing: 'easeOutQuint', duration: 600, queue: false });
                        $(".authent").animate({ opacity: 1 }, { duration: 200, queue: false }).addClass('visible');
                        //setTimeout(function () {
                        //    $('.login').addClass('testtwo');
                        //}, 800);
                        //setTimeout(function () {
                        //    $(".authent").show().animate({ right: -320 }, { easing: 'easeOutQuint', duration: 600, queue: false });
                        //    $(".authent").animate({ opacity: 1 }, { duration: 200, queue: false }).addClass('visible');
                        //}, 1000);

                        $.ajax({
                            type: "POST",
                            url: "/MyHome/LoginChecking",
                            data: '{user: "' + $("#InputUsername").val() + '",passw: "' + $("#InputPswrd").val() + '"}',
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function (responsent) {
                                //sessionStorage.setItem("mychecklogint", responsent.Username);
                                //alert(sessionStorage.getItem("mychecklogint"));
                                if (responsent.Username != null) {
                                    sessionStorage.setItem("mychecklogint", "Values");
                                    //alert(sessionStorage.setItem("mychecklogint", "Values"));
                                    $(".authent").show().animate({ right: -500 }, { easing: 'easeOutQuint', duration: 600, queue: false });
                                    $(".authent").animate({ opacity: 0 }, { duration: 200, queue: false }).addClass('visible');
                                    $('.login').removeClass('testtwo');
                                    $('.login').removeClass('test');
                                    $('.login div').fadeOut(123);
                                    $('.login div').fadeIn();
                                    MyGetUrl();
                                }
                                else if (responsent.Username == null) {
                                    sessionStorage.setItem("mychecklogint", "Empty");
                                    //alert(sessionStorage.setItem("mychecklogint", "Empty"));
                                    $(".authent").show().animate({ right: -500 }, { easing: 'easeOutQuint', duration: 600, queue: false });
                                    $(".authent").animate({ opacity: 0 }, { duration: 200, queue: false }).addClass('visible');
                                    $('.login').removeClass('testtwo');
                                    $('.login').removeClass('test');
                                    $('.login div').fadeOut(123);
                                    $('.login div').fadeIn();
                                    MyGetUrl();
                                }
                            },
                            error: function (responsent) {
                                //alert("Error");
                                //$("#ErrorMessage").css("display", "block");

                            }
                        });

                        //setTimeout(function () {
                        //    $(".authent").show().animate({ right: -500 }, { easing: 'easeOutQuint', duration: 600, queue: false });
                        //    $(".authent").animate({ opacity: 0 }, { duration: 200, queue: false }).addClass('visible');
                        //    $('.login').removeClass('testtwo');
                        //}, 10000);
                        //setTimeout(function () {
                        //    $('.login').removeClass('test');
                        //    $('.login div').fadeOut(123);
                        //}, 10000);
                        //setTimeout(function () {
                        //    $('.login div').fadeIn();

                        //}, 10000);

                    }
                }

                function MyGetUrl() {
                    //alert(sessionStorage.getItem("mychecklogin"));
                    //var mycheck = sessionStorage.getItem("mychecklogint");
                    //alert(sessionStorage.getItem("mychecklogint"));
                    if (sessionStorage.getItem("mychecklogint") == "Empty") {
                        //alert("null show");
                        $("#ErrorMessage").css("display", "block");
                    }
                    else if (sessionStorage.getItem("mychecklogint") == "Values") {
                        //alert("do not null");
                        $("#ErrorMessage").css("display", "none");
                        //sessionStorage.setItem("username", mycheck);
                        location.href = "@Url.Action("HomePageLoad", "MyHome")";
                    }
                }
            });
            @*$(document).ready(function () {
                window.onload = function () {
                    var adiistif = '@Session["username"]';
                    if (adiistif == null) {
                        window.history.pushState(null, "", window.location.href);
                        window.onpopstate = function () {
                            window.history.pushState(null, "", window.location.href);
                        }
                    }
                }
            });*@
    </script>
    <style type="text/css">
        .Usernamecls {
            border-bottom: 2px solid #b50e0e;
        }

        .Passwcls {
            border-bottom: 2px solid #b50e0e;
        }
    </style>

</body>
</html>
