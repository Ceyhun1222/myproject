<Window x:Class="Aran.Delta.Settings.SettingsArenaWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:settings="clr-namespace:Aran.Delta.Settings"
             mc:Ignorable="d" 
             SizeToContent="WidthAndHeight"
            Title="Delta Settings"
             >
    <Grid>
        <StackPanel Orientation="Vertical">
            <settings:SettingsForm x:Name="StFrom" Margin="5 -10 5 5" Loaded="StFrom_Loaded"/>
            <Button Margin="0,10,10,4" Content="OK" Click="ButtonBase_OnClick" Width="80" Height="24" HorizontalAlignment="Right"/>
        </StackPanel>
    </Grid>
</Window>
