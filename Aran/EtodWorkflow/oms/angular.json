{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "oms": {
      "root": "src",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/oms",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.png",
              "src/assets",
              {
                "glob": "**/*",
                "input": "global_assets/",
                "output": "src/assets"
              }
            ],
            "styles": [
              "src/styles.css",
              "global_assets/limitless/global_assets/css/icons/icomoon/styles.css",
              "global_assets/limitless/seed/assets/css/bootstrap.min.css",
              "global_assets/limitless/seed/assets/css/bootstrap_limitless.min.css",
              "global_assets/limitless/seed/assets/css/layout.min.css",
              "global_assets/limitless/seed/assets/css/colors.min.css",
              "global_assets/plugins/ol/v4.6.5/ol.css",
              "global_assets/limitless/assets/css/components.min.css"
            ],
            "scripts": [
              "global_assets/limitless/global_assets/js/main/jquery.min.js",
              "global_assets/limitless/global_assets/js/main/bootstrap.bundle.min.js",
              "global_assets/limitless/global_assets/js/plugins/loaders/blockui.min.js",
              "global_assets/limitless/global_assets/js/plugins/forms/selects/select2.min.js",
              "global_assets/plugins/proj4js/dist/proj4.js",
              "global_assets/plugins/ol/v4.6.5/ol.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "projects/omslib/src/lib/_config/_dev.ts",
                  "with": "projects/omslib/src/lib/_config/_prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "oms:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "oms:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "oms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "oms-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "oms:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "oms:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client": {
      "root": "projects/client/",
      "sourceRoot": "projects/client/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/client",
            "index": "projects/client/src/index.html",
            "main": "projects/client/src/main.ts",
            "polyfills": "projects/client/src/polyfills.ts",
            "tsConfig": "projects/client/tsconfig.app.json",
            "assets": [
              "projects/client/src/favicon.ico",
              "projects/client/src/assets",
              {
                "glob": "**/*",
                "input": "global_assets/",
                "output": "projects/client/src/assets"
              }
            ],
            "styles": [
              "src/styles.css",
              "global_assets/limitless/global_assets/css/icons/icomoon/styles.css",
              "global_assets/limitless/seed/assets/css/bootstrap.min.css",
              "global_assets/limitless/seed/assets/css/bootstrap_limitless.min.css",
              "global_assets/limitless/seed/assets/css/layout.min.css",
              "global_assets/limitless/seed/assets/css/colors.min.css",
              "global_assets/plugins/ol/v4.6.5/ol.css",
              "global_assets/limitless/assets/css/components.min.css"
            ],
            "scripts": [
              "global_assets/limitless/global_assets/js/main/jquery.min.js",
              "global_assets/limitless/global_assets/js/main/bootstrap.bundle.min.js",
              "global_assets/limitless/global_assets/js/plugins/loaders/blockui.min.js",
              "global_assets/limitless/global_assets/js/plugins/forms/selects/select2.min.js",
              "global_assets/plugins/proj4js/dist/proj4.js",
              "global_assets/plugins/ol/v4.6.5/ol.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/client/src/environments/environment.ts",
                  "with": "projects/client/src/environments/environment.prod.ts"
                },
                {
                  "replace": "projects/omslib/src/lib/_config/_dev.ts",
                  "with": "projects/omslib/src/lib/_config/_prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/client/src/test.ts",
            "polyfills": "projects/client/src/polyfills.ts",
            "tsConfig": "projects/client/tsconfig.spec.json",
            "karmaConfig": "projects/client/karma.conf.js",
            "styles": [
              "projects/client/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/client/src/favicon.ico",
              "projects/client/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/client/tsconfig.app.json",
              "projects/client/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-e2e": {
      "root": "projects/client-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/client-e2e/protractor.conf.js",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/client-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "omslib": {
      "root": "projects/omslib",
      "sourceRoot": "projects/omslib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/omslib/tsconfig.lib.json",
            "project": "projects/omslib/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/omslib/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/omslib/src/test.ts",
            "tsConfig": "projects/omslib/tsconfig.spec.json",
            "karmaConfig": "projects/omslib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/omslib/tsconfig.lib.json",
              "projects/omslib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "oms"
}