<Window x:Class="UserManager.View.Extended.MainManagementWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:Extended="clr-namespace:UserManager.View.Extended" 
        Title="Management Console" Height="427" Width="795" 
        WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <TabControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <TabItem Header="Users">
                <Extended:UserManagerWindow/>
            </TabItem>
            <TabItem Header="Access">
                <Extended:AccessManagerWindow/>
            </TabItem>
           
        </TabControl>
    </Grid>
</Window>
